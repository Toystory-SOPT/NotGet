<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.toystory.notget">

    <application
        android:name=".NotGetApplication"
        android:icon="@mipmap/logo_notget"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_notget_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NotGet">
        <activity android:name=".view.CreateActivity"></activity>
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>